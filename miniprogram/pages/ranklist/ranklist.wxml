<view class="background">
	<image class="background-image" src="../../images/rankList.png" mode="widthFix" />
	<view class="first">
		<image class="avatar" src="{{weekRankList[0].avatarUrl}}"></image>
		<view class="nickname">{{weekRankList[0].nickName}}</view>
		<view class="value">{{weekRankList[0].totalValue}}</view>
	</view>
	<view class="second">
		<image class="avatar" src="{{weekRankList[1].avatarUrl}}"></image>
		<view class="nickname">{{weekRankList[1].nickName}}</view>
		<view class="value">{{weekRankList[1].totalValue}}</view>
	</view>
	<view class="third">
		<image class="avatar" src="{{weekRankList[2].avatarUrl}}"></image>
		<view class="nickname">{{weekRankList[2].nickName}}</view>
		<view class="value">{{weekRankList[2].totalValue}}</view>
	</view>
	<view class="update-date">更新时间：{{updateDate}}</view>
</view>

<tabbar class="tabbar" tabbar="{{tabbar}}" bindtabbarChange="tabbarChange">
</tabbar>

<swiper style="height:{{wholeRankHeight?wholeRankHeight+'px':'auto'}}" current="{{chosenTabIndex}}"
	class="swiper-region" bindchange="swiperChange">

	<swiper-item class="swiper-item">
		<view class="whole-rank">
			<view class="whole-rank-item" wx:for="{{dayRankList}}" wx:key="index">
				<view class="left">
					<text class="rank-number">{{index+1}}</text>
					<image class="user-avatar" src="{{item.avatarUrl}}"></image>
					<view class="user-nickname">{{item.nickName}}</view>
				</view>
				<view class="right">
					<view class="juan-number">{{item.totalValue}}</view>
					<image class="juan-image" src="../../icon/roll2.png"></image>
				</view>
			</view>
		</view>
	</swiper-item>

	<swiper-item class="swiper-item">
			<view class="whole-rank">
				<view class="whole-rank-item" wx:for="{{weekRankList}}" wx:key="index">
					<view class="left">
						<text class="rank-number">{{index+1}}</text>
						<image class="user-avatar" src="{{item.avatarUrl}}"></image>
						<view class="user-nickname">{{item.nickName}}</view>
					</view>
					<view class="right">
						<view class="juan-number">{{item.totalValue}}</view>
						<image class="juan-image" src="../../icon/roll2.png"></image>
					</view>
				</view>
			</view>
		</swiper-item>

