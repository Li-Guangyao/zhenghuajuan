<view class="my-rank">
	<view class="left">
		<image class="user-avatar" src="{{userInfo.avatarUrl}}"></image>
		<view class="user-nickname">{{userInfo.nickName}}</view>
	</view>
	<view class="right">
		<text class="rank-info" wx:if="{{myRank!=-1}}">{{"No."+ myRank}}</text>
		<text class="rank-info" wx:if="{{myRank==-1}}">您未上榜</text>
	</view>
</view>